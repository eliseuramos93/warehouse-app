<%= form_with model: @warehouse do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %> <br />
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %> <br />
  </div>
  <div>
    <%= f.label :code %>
    <%= f.text_field :code %> <br />
  </div>
  <div>
    <%= f.label :address %>
    <%= f.text_field :address %> <br />
  <div>
  <div>
    <%= f.label :cep %>
    <%= f.text_field :cep %> <br />
  <div>
    <%= f.label :city %>
    <%= f.text_field :city %> <br />
  </div>
  <div>
    <%= f.label :area %>
    <%= f.number_field :area %> <br />
  </div>

  <%= f.submit 'Enviar' %>
<% end %>